<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.myapplication.drawyourcity.app.ChoosePackage"
    android:background="@drawable/background_redlist">


    <ImageButton
    android:id="@+id/icecream"
    android:layout_height="49dp"
    android:layout_width="247dp"
    android:background="@drawable/ic_icecreamr"
    android:layout_centerHorizontal="true"
    android:visibility="gone"
    android:layout_marginTop="15dp"/>

    <ImageButton
    android:id="@+id/ph_med_1"
    android:layout_height="49dp"
    android:layout_width="247dp"
    android:background="@drawable/placeholder_med_1"
    android:layout_centerHorizontal="true"
    android:visibility="gone"
    android:layout_marginTop="15dp"
    android:layout_below="@+id/icecream"/>

    <ImageButton
        android:id="@+id/ph_med_2"
        android:layout_height="49dp"
        android:layout_width="247dp"
        android:background="@drawable/placeholder_med_2"
        android:layout_centerHorizontal="true"
        android:visibility="gone"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/ph_med_1"/>

    <ImageButton
        android:id="@+id/ph_med_3"
        android:layout_height="49dp"
        android:layout_width="247dp"
        android:background="@drawable/placeholder_med_3"
        android:layout_centerHorizontal="true"
        android:visibility="gone"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/ph_med_2"/>

    <ImageButton
        android:id="@+id/pudding"
        android:layout_centerHorizontal="true"
        android:layout_height="49dp"
        android:layout_width="160dp"
        android:background="@drawable/ic_puddingr"
        android:visibility="gone"
        android:layout_marginTop="15dp"/>

    <ImageButton
        android:id="@+id/fish"
        android:layout_height="42dp"
        android:layout_width="390dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/ic_fishr"
        android:visibility="gone"
        android:layout_marginTop="15dp"/>

</RelativeLayout>
